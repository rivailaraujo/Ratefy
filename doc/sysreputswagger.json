      }
    }
  ],
  "schemes": [
    "https",
    "http"
  ],
  "paths": {
    "/ratings": {
      "post": {
        "tags": [
          "ratings"
        ],
        "summary": "adicionando uma avaliaçao",
        "description": "",
        "operationId": "addRate",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "Objeto necessário",
            "required": true,
            "schema": {
              "$ref": "#/definitions/Rating"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Entrada Inválida Ou Nota inválida para tipo de avaliação"
          },
          "403": {
            "description": "Chave Inválida (Parâmetro key)"
          },
          "500": {
            "description": "Erro Interno do Servidor"
          }
        }
      }
    },
    "/ratings/{YOUR_KEY}": {
      "get": {
        "tags": [
          "ratings"
        ],
        "summary": "Status geral das avaliações por aplicação",
        "description": "Retorna detalhes gerais do uso das avaliações",
        "operationId": "getStatusGeral",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "YOUR_KEY",
            "in": "path",
            "description": "A chave de aplicação é necessária para conseguir os status gerais de avaliações. Use \"2524f98231582c9395a1971fc66cfae5\" para testar.",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Operação feita com sucesso",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Status"
              }
            }
          },
          "403": {
            "description": "Chave Inválida (YOUR_KEY)"
          }
        }
      }
    },
    "/ratings/{YOUR_KEY}/{TYPE_RATE}": {
      "get": {
        "tags": [
          "ratings"
        ],
        "summary": "Status geral das avaliações por aplicação e tipo de avaliação",
        "description": "Retorna detalhes de todas as avaliações feitas com determinado tipo.",
        "operationId": "getStatusTipo",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "YOUR_KEY",
            "in": "path",
            "description": "A chave de aplicação é necessária para conseguir os status gerais de avaliações. Use \"2524f98231582c9395a1971fc66cfae5\" para testar.",
            "required": true,
            "type": "string"
          },
          {
            "name": "TYPE_RATE",
            "in": "path",
            "description": "O tipo de avaliação necessário para conseguir os status gerais de avaliações. Use \"1\" para five-stars, \"2\" para  para testar like-dislike e \"3\" para slider.",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Operação feita com sucesso",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/StatusTipo"
              }
            }
          },
          "400": {
            "description": "Tipo Inválido (TYPE_RATE)"
          },
          "403": {
            "description": "Chave Inválida (YOUR_KEY)"
          }
        }
      }
    },
    "/items/{YOUR_KEY}/{ITEM_ID}/{TYPE}": {
      "get": {
        "tags": [
          "items"
        ],
        "summary": "Detalhes das avaliações por item e tipo de avaliação específica.",
        "description": "Multiple status values can be provided with comma separated strings",
        "operationId": "findPetsByStatus",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "YOUR_KEY",
            "in": "path",
            "description": "A chave de aplicação é necessária para conseguir os status gerais de avaliações. Use \"2524f98231582c9395a1971fc66cfae5\" para testar.",
            "required": true,
            "type": "string"
          },
          {
            "name": "ITEM_ID",
            "in": "path",
            "description": "O id de item é necessário. Use \"1\" para testar.",
            "required": true,
            "type": "string"
          },
          {
            "name": "TYPE",
            "in": "path",
            "description": "Filtrar o resultado por tipo de avaliação (1 para Five-Star, 2 para Like-Dislike e 3 para Slider)",
            "required": true,
            "type": "integer",
            "enum": [
              1,
              2,
              3
            ],
            "default": 1
          }
        ],
        "responses": {
          "200": {
            "description": "sucesso",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Items"
              }
            }
          },
          "400": {
            "description": "item inválido (ITEM_ID), tipo Inválido(TYPE) ou tipo de avaliação não encontrado para o item."
          },
          "403": {
            "description": "chave inválida (YOUR_KEY)"
          }
        }
      }
    }
  },
